{
  "name": "Ally Linux Bill Pay Fixer",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "This fixes the Bill Pay page for Ally when using Linux",
  "background": {
    "scripts": ["background.js"]
  },
  "homepage_url": "http://github.com/blackrobot/ally-chrome-linux-extension",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*.ally.com/",
    "https://*.ally.com/"
  ]
}
